</head>

<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">

    <div class="site-wrap">

        <div class="site-mobile-menu site-navbar-target">
            <div class="site-mobile-menu-header">
                <div class="site-mobile-menu-close mt-3">
                    <span class="icon-close2 js-menu-toggle"></span>
                </div>
            </div>
            <div class="site-mobile-menu-body"></div>
        </div>


        <div class="py-2 bg-light">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-9 d-none d-lg-block">
                        <a href="#" class="small mr-3"><span class="icon-question-circle-o mr-2"></span> Hỏi Về ThyPC?</a>
                        <a href="#" class="small mr-3"><span class="icon-phone2 mr-2"></span> 0846 772 473
                        <a href="#" class="small mr-3"><span class="icon-envelope-o mr-2"></span> nguyendinhthy12032002@gmail.com</a>
                    </div>
                    <div class="col-lg-3 text-right">
                        <a href="#" class="widget-view">

                            @if (Session["idUser"] != null)
                            {
                                @Html.ActionLink("Admin", "Admin", "Admin", new { area = "Admin" }, new { @class = "fas fa-user-shield" }) 
                                @Html.ActionLink("Đăng xuất", "Login", "Home", new { @class = "fas fa-sign-out-alt" }, null)
                            }
                            else
                            {
                                 @Html.ActionLink("Đăng nhập", "Login", "Home", new { @class = "icon-unlock-alt" }, null)

                                 @Html.ActionLink("Đăng ký", "Register", "Home", new { @class = "icon-users" }, null)


                                    <a href="@Url.Action("Index", "Cart")" class="widget-view">
                                        <div class="icon-area">
                                            <i class="fa fa-shopping-cart"></i>
                                            @if (@Session["count"] == null)
                                            {
                                                <span class="notify" id="CartCount">0</span>
                                            }
                                            else
                                            {
                                                <span class="notify" id="CartCount">@Session["count"]</span>
                                            }
                                        </div>
                                        <small class="text"> Giỏ Hàng </small>
                                    </a>
                            }
</div>
                </div>
            </div>
        </div>
        <header class="site-navbar py-4 js-sticky-header site-navbar-target" role="banner">

            <div class="container">
                <div class="d-flex align-items-center">
                    <div class="site-logo">
                        <a href="@Url.Action("Index","Register")" class="d-block">
                            <img src="~/Controllers/images/logo.jpg" alt="Image" class="img-fluid">
                        </a>
                    </div>
                    <div class="mr-auto">
                        <nav class="site-navigation position-relative text-right" role="navigation">
                            <ul class="site-menu main-menu js-clone-nav mr-auto d-none d-lg-block">
                                <li class="active">
                                    @*<a href="index.html" class="nav-link text-left">Home</a>*@
                                    @Html.ActionLink("Trang Chủ", "Index", "Home", new { @class = "nav-link text-left" }, null)

                                </li>
                                <li class="has-children">
                                    @*<a href="about.html" class="nav-link text-left">About Us</a>*@
                                    @Html.ActionLink("Danh Sách Sản Phẩm", "Index", "About", new { @class = "nav-link text-left" }, null)

                                    <ul class="dropdown">
                                        <li><a href="teachers.html">Link Kiện Máy Tính</a></li>
                                        <li><a href="about.html">Điện Thoại</a></li>

                                    </ul>
                                </li>
                                <li>
                                    @* <a href="admissions.html" class="nav-link text-left">Admissions</a>*@
                                    @Html.ActionLink("ThyPC", "Index", "Admissions", new { @class = "nav-link text-left" }, null)

                                </li>
                                <li>
                                    @*@ <a href="courses.html" class="nav-link text-left">Courses</a>*@
                                    @Html.ActionLink("Thông Tin", "Index", "Courses", new { @class = "nav-link text-left" }, null)

                                </li>
                                <li>
                                    @* <a href="contact.html" class="nav-link text-left">Contact</a>*@
                                    @Html.ActionLink("Liên Hệ", "Index", "Contact", new { @class = "nav-link text-left" }, null)

                               

                               </li>
                            </ul>                                                                                                                                                                                                                                                                                          </ul>
                        </nav>

                    </div>
                    <div class="ml-auto">
                        <div class="social-wrap">
                            <a href="https://www.facebook.com/NDT.12032002"><span class="icon-facebook"></span></a>
